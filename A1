def check_nums(rc):
    result = True
    for i in rc:
        if i not in range (1,len(rc)+1):
            result = result and False
    return result


def check_rc(rc):
    adlist = []
    for i in rc:
        if i not in adlist:
            adlist.append(i)
            if len(adlist) == len(rc):
                return True
        else:
            return False
            
def comp_check(rc):
    return check_nums(rc) and check_rc(rc)

def check_sudoku(sudoku):
    result = [True]
    columns = [[] for x in range (len(sudoku))]
    for i in sudoku:
        result = result and comp_check(i)
        for x in range (0,len(sudoku)):
            columns[x].append(i[x])
    for i in columns:
        result = result and check_rc(i)
    return result
